{
    "compilerOptions": {
        "moduleResolution": "node",
        "module": "commonjs",
        "lib": [
            "es6"
        ],
        "noImplicitAny": true,
        "noImplicitThis": false,
        "strictNullChecks": false,
        "strictFunctionTypes": true,
        "baseUrl": "../",
        "typeRoots": [
            "../node_modules/@types",
            "../"
        ],
        "types": ["node", "geojson"],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true,
        "allowJs": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "skipLibCheck": true
    },
    "files": [
        "./index.d.ts",
        "./callback.d.ts",
        "mariadb-tests.ts",
        "mariadb-tests-callback.ts"
    ]
}